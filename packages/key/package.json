{
  
  "name": "@ep2/key",
  "version": "1.0.0",
  "private": false,
  "description": "Tools for secure communication between peers",
  "typings": "out/index.d.ts",
  "source": "src/index.ts",
  "main": "out/cjs/index.js",
  "module": "out/esm/index.js",
  "type": "commonjs",
  "files": [
    "out/**/*"
  ],
  "bin": {
    "generateKey": "./out/index.js"
  },
  "exports2": {
    ".": {
      "require": "./out/cjs/index.js",
      "import": "./out/esm/index.js"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./out/index.d.ts",
        "default": "./out/esm/index.js"
      },
      "require": {
        "types": "./out/index.d.ts",
        "default": "./out/cjs/index.js"
      }
    }
  },
  "targets": {
    "binary": {
      "source": "bin/index.ts"
    },
    "main": {},
    "module": {}
  },
  "keywords": [
    "ep2",
    "peerjs",
    "libsodium",
    "hybrid",
    "security",
    "encrypt",
    "sign",
    "handshake"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bosskabouter/ep2.git"
  },
  "scripts": {
    "build": "tsc & tsc --module es2015 --outDir out/esm",
    "coverage": "jest --coverage",
    "dev": "tsc --watch & tsc --watch  --module es2015 --outDir out/esm",
    "test": "jest"
  },
  "author": "mailto:paul.kuit@protonmail.com",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/jest": "^29.5.1",
    "@types/libsodium-wrappers": "^0.7.10",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "libsodium-wrappers": "^0.7.11"
  },
  "$schema": "https://json.schemastore.org/package"
}